{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "amazon-product/block",
    "version": "1.0.0",
    "title": "Amazon Product",
    "category": "embed",
    "icon": "cart",
    "description": "Display an Amazon product with image, title, price, and buy button",
    "keywords": ["amazon", "product", "affiliate", "ecommerce"],
    "supports": {
      "html": false,
      "align": false
    },
    "attributes": {
      "productUrl": {
        "type": "string",
        "default": ""
      },
      "productData": {
        "type": "object",
        "default": {}
      }
    },
    "textdomain": "amazon-product-block",
    "editorScript": "file:./build/index.js",
    "editorStyle": "file:./build/index.css",
    "style": "file:./build/style-index.css"
  }